<h1>Login</h1>
<%= form_with url: sessions_path, method: :post do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %> <br>
    <%= f.label :password %>
    <%= f.password_field :password %><br>
    <%= f.label :professor %>
    <%= f.radio_button :professor %> <br>

    <%= f.submit "Login" %>
<% end %>